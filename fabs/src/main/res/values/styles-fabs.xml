<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Widget.Vitamin.FloatingActionButton" parent="Widget.MaterialComponents.FloatingActionButton">
        <item name="backgroundTint">@color/vtmn_fabs_container</item>
        <item name="rippleColor">?attr/vtmnActiveColorBrand</item>
        <item name="tint">@color/vtmn_fabs_icon</item>
        <item name="fabSize">normal</item>
    </style>

    <style name="Widget.Vitamin.MiniFloatingActionButton" parent="Widget.Vitamin.FloatingActionButton">
        <item name="fabSize">mini</item>
    </style>

    <style name="Widget.Vitamin.ExtendedFloatingActionButton" parent="Widget.MaterialComponents.ExtendedFloatingActionButton">
        <item name="backgroundTint">@color/vtmn_fabs_container</item>
        <item name="rippleColor">?attr/vtmnActiveColorBrand</item>
        <item name="iconTint">@color/vtmn_fabs_icon</item>
        <item name="android:textColor">@color/vtmn_fabs_text</item>
    </style>

</resources>